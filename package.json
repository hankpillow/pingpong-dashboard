    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.13.0",
    "node-fetch": "^1.7.3",
    "nodemon": "^1.12.1",
    "ramda": "^0.25.0"
